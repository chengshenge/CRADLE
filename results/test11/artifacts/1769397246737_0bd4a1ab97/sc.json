      "purpose": "Subtract a list of counts from a total count to determine the remaining count.",
      "inputs": [
        "total",
        "subtractions"
      ],
      "outputs": "int",
      "edge_cases": [
        "Negative results",
        "Empty subtractions list"
      ],
      "tests": [
        {
          "input": "(10, [3, 2])",
          "expect": "5"
        },
        {
          "input": "(5, [])",
          "expect": "5"
