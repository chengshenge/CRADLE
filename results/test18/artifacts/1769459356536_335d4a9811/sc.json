{
  "selected_skills": [],
  "new_skill_specs": [
    {
      "name": "interpret_diagram_for_angle",
      "signature": "(diagram_data)->angle_measure",
      "purpose": "Extract and interpret the measure of a specified angle from a given diagram.",
      "inputs": [
        "diagram_data"
      ],
      "outputs": "angle_measure",
      "edge_cases": [
        "Diagram is unclear or labels are ambiguous",
        "Multiple angles are marked with similar labels"
      ],
      "tests": [
        {
          "input": "diagram_data_with_angle_97",
          "expect": 97
        },
        {
          "input": "diagram_data_with_angle_102",
          "expect": 102
        }
      ],
      "tags": [
        "geometry",
        "diagram interpretation",
        "angle measurement"
      ]
    },
    {
      "name": "map_angle_to_option",
      "signature": "(angle_measure, options)->option_label",
      "purpose": "Map a given angle measure to the corresponding multiple-choice option label.",
      "inputs": [
        "angle_measure",
        "options"
      ],
      "outputs": "option_label",
      "edge_cases": [
        "Angle measure does not match any option",
        "Multiple options have the same measure"
      ],
      "tests": [
        {
          "input": {
            "angle_measure": 97,
            "options": [
              {
                "label": "A",
                "text": "97"
              },
              {
                "label": "B",
                "text": "102"
              }
            ]
          },
          "expect": "A"
        },
        {
          "input": {
            "angle_measure": 102,
            "options": [
              {
                "label": "A",
                "text": "97"
              },
              {
                "label": "B",
                "text": "102"
              }
            ]
          },
          "expect": "B"
        }
      ],
      "tags": [
        "multiple choice",
        "option mapping",
        "geometry"
      ]
    }
  ],
  "skill_priority": [
    "interpret_diagram_for_angle",
    "map_angle_to_option"
  ]
}