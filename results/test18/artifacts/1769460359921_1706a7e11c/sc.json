      "signature": "(image_hash:str, object_type:str, position_constraint:Optional[str])->int",
      "purpose": "Count the number of specified objects in an image, optionally considering their position relative to another object.",
      "inputs": [
        "image_hash",
        "object_type",
        "position_constraint"
      ],
      "outputs": "int",
      "edge_cases": [
