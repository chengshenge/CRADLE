{
  "selected_skills": [],
  "new_skill_specs": [
    {
      "name": "identify_and_count_objects",
      "signature": "(image_hash:str, object_attributes:dict)->int",
      "purpose": "Identify and count objects in an image based on specified attributes.",
      "inputs": [
        "image_hash",
        "object_attributes"
      ],
      "outputs": "int",
      "edge_cases": [
        "Objects partially obscured",
        "Objects with similar attributes but not exact match"
      ],
      "tests": [
        {
          "input": "{\"image_hash\": \"1c3ef3d55405\", \"object_attributes\": {\"color\": \"gray\", \"material\": \"rubber\", \"position\": \"in front of small yellow aeroplane\"}}",
          "expect": "3"
        },
        {
          "input": "{\"image_hash\": \"1c3ef3d55405\", \"object_attributes\": {\"size\": \"big\", \"color\": \"cyan\", \"finish\": \"matte\", \"type\": \"fighter\"}}",
          "expect": "2"
        }
      ],
      "tags": [
        "image processing",
        "object detection",
        "counting"
      ]
    },
    {
      "name": "compare_counts",
      "signature": "(count1:int, count2:int)->str",
      "purpose": "Compare two counts and return the corresponding multiple-choice option.",
      "inputs": [
        "count1",
        "count2"
      ],
      "outputs": "str",
      "edge_cases": [
        "Counts are equal",
        "One or both counts are zero"
      ],
      "tests": [
        {
          "input": "{\"count1\": 3, \"count2\": 2}",
          "expect": "\"A\""
        },
        {
          "input": "{\"count1\": 1, \"count2\": 2}",
          "expect": "\"B\""
        }
      ],
      "tags": [
        "comparison",
        "multiple-choice"
      ]
    }
  ],
  "skill_priority": [
    "identify_and_count_objects",
    "compare_counts"
  ]
}